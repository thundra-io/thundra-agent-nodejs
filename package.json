{
  "name": "@thundra/core",
  "version": "2.12.5",
  "description": "Thundra Lambda Node.js Agent",
  "main": "dist/thundra.js",
  "scripts": {
    "compile": "tsc",
    "build": "rollup -c",
    "start": "npm-run-all --parallel watch:server watch:build",
    "test-automatic-instrumentation": "node ./test/instrumentation/automated.instrumentation.js",
    "test": "npm run compile && npm run test-automatic-instrumentation && docker-compose up -d && jest --silent; docker-compose stop",
    "test-ci": "npm run compile && npm run test-automatic-instrumentation && jest --silent --config jest.ci.config.json --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "tslint --project tsconfig.json -t stylish",
    "release-minor": "npm install; npm run build; release-it minor -n --npm.publish --src.commit --src.tag --src.push --src.tagName='v%s' --github.release --no-requireCleanWorkingDir --no-requireUpstream",
    "release-minor-without-tag": "npm install; npm run build; release-it minor -n --npm.publish --no-requireCleanWorkingDir --no-requireUpstream --no-src.commit --no-src.tag --no-src.push --no-src.push --no-src.tagName='v%s' --no-github.release",
    "release": "npm install; npm run build; release-it -n --npm.publish --src.commit --src.tag --src.push --no-src.push --src.tagName='v%s' --github.release --no-requireCleanWorkingDir --no-requireUpstream",
    "release-without-tag": "npm install; npm run build; release-it -n --npm.publish --no-requireCleanWorkingDir --no-requireUpstream --no-src.commit --no-src.tag --no-src.push --no-src.push --no-src.tagName='v%s' --no-github.release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thundra-io/thundra-lambda-agent-nodejs.git"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "observability",
    "tracing",
    "metrics",
    "analytics",
    "telemetry",
    "opentracing"
  ],
  "author": "Thundra <info@thundra.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/thundra-io/thundra-lambda-agent-nodejs/issues"
  },
  "homepage": "https://github.com/thundra-io/thundra-lambda-agent-nodejs#readme",
  "devDependencies": {
    "@types/minimatch": "^3.0.3",
    "@types/node": "^10.5.0",
    "@types/uuid": "^3.4.3",
    "aws-sdk": "^2.610.0",
    "aws-xray-sdk-core": "^2.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.4.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "coveralls": "^3.0.1",
    "elasticsearch": "^15.4.1",
    "eslint": "^4.19.1",
    "eslint-plugin-babel": "^5.1.0",
    "express": "^4.17.1",
    "gaze-run-interrupt": "^1.0.1",
    "i": "^0.3.6",
    "ioredis": "^4.10.0",
    "jest": "^22.4.4",
    "mongodb": "^3.2.4",
    "mysql": "^2.16.0",
    "mysql2": "2.1.0",
    "nock": "^10.0.6",
    "npm-run-all": "^4.1.1",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "redis": "^2.8.0",
    "release-it": "^7.5.0",
    "rollup": "^1.16.4",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.1.0",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "sequelize": "^5.21.1",
    "supertest": "^4.0.2",
    "ts-loader": "^4.4.1",
    "ts-node": "^7.0.0",
    "tslint": "5.7.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@thundra/instrumenter": "^1.3.1",
    "@thundra/warmup": "1.0.0",
    "fast-copy": "^2.1.0",
    "koalas": "^1.0.2",
    "lodash.flatten": "^4.4.0",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.trim": "^4.5.1",
    "md5": "^2.2.1",
    "module-details-from-path": "^1.0.3",
    "object-sizeof": "^1.6.1",
    "opentracing": "0.14.1",
    "require-in-the-middle": "^3.1.0",
    "semver": "^5.5.0",
    "shimmer": "^1.2.0",
    "uuid": "^3.2.1",
    "ws": "^7.2.1"
  }
}
