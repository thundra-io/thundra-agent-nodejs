  "moduleFileExtensions": [
    "js"
  ],
  "collectCoverageFrom": [
    "dist/*.js",
    "dist/opentracing/*.js",
    "dist/opentracing/instrument/*.js",
    "dist/opentracing/sampler/*.js",
    "dist/plugins/*.js",
